    "@nuxt/scripts": "^0.4.3",
    "@nuxtjs/algolia": "^1.10.2",
    "@nuxtjs/critters": "^0.7.1",
    "@nuxtjs/device": "^3.1.1",
    "@nuxtjs/fontaine": "^0.4.2",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/i18n": "^8.3.1",
    "@nuxtjs/seo": "^2.0.0-rc.10",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "@pinia/nuxt": "^0.5.1",
    "@storefront-ui/typography": "^2.6.1",
    "@storefront-ui/vue": "^2.6.3",
    "@vue-storefront/sdk": "^2.0.0",
    "@vueuse/nuxt": "^10.9.0",
